@model ProyectoCVA.ViewModels.EstudiantesVM.PerfilViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = Model.Title;
}
<head>

    
    <link rel="stylesheet" href="~/Views/lib/FontAwesome/fontawesome/web-fonts-with-css/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/Views/lib/bootstrap-4.0.0/dist/css/bootstrap.min.css">
    <link href="wwwroot/css/Perfil.css" rel="stylesheet" tipe="text/css"/>
    @*<link href="@Url.IsLocalUrl("~/Content/Perfil.css")" rel="stylesheet" tipe="text/css" />*@


</head>
<body>


    <nav class="navbar navbar-expand-md bg-dark navbar-dark text-center text-uppercase">
        <a class="navbar-brand" href="#">Perfil</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar" aria-controls="navbar" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse text-center  " id="navbar">
            <ul class="navbar-nav mr-auto mt-2 mt-lg-0"></ul>
            <a href="../HTML/Menu.html" target="_self" class="btn btn-primary " role="button">Regresar</a>

        </div>
    </nav>
    <div class="container">
        <div>
            <table class="table">

                <tbody>
                    @foreach (var usuario in Model.ListaUsuarios)
                    {

                        <tr>
                            <th scope="row">Nombre</th>
                            <td>@usuario.Nombre</td>

                        </tr>

                        <tr>
                            <th scope="row">Apellidos</th>
                            <td>@usuario.Apellidos</td>

                        </tr>
                        <tr>
                            <th scope="row">Fecha de Nacimiento</th>
                            <td>@usuario.FechaNacimiento</td>

                        </tr>

                        <tr>
                            <th scope="row">Genero</th>
                            <td>@usuario.Genero</td>

                        </tr>
                        <tr>
                            <th scope="row">Cedula</th>
                            <td>@usuario.Cedula</td>

                        </tr>
                        <tr>
                            <th scope="row">Carnet</th>
                            <td>@usuario.Carnet</td>

                        </tr>
                        <tr>
                            <th scope="row">Correo Interno</th>
                            <td>@usuario.CorreoUniversitario</td>

                        </tr>
                        <tr>
                            <th scope="row">Correo Personal</th>
                            <td>@usuario.CorreoPersonal</td>

                        </tr>
                        <tr>
                            <th scope="row">Telefono</th>
                            <td>@usuario.Telefono</td>

                        </tr>


                    }

                </tbody>
            </table>
        </div>
    </div>
    <script src="~/Views/lib/JQuery/jquery-3.3.1.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>



